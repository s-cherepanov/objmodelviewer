/*======================================== HEADERS =======================================*/

#include "global.h"

#define VOS_NT_WINDOWS32    0x00040004L
#define VFT_APP             0x00000001L

/*======================================= RESOURCES ======================================*/

A ICON MOVEABLE PURE LOADONCALL DISCARDABLE APP_FILEICON

1 VERSIONINFO
FILEVERSION     APP_VERSION
PRODUCTVERSION  APP_VERSION
FILETYPE        VFT_APP
{
  BLOCK "StringFileInfo"
	 {
		 BLOCK "040904E4"
		 {
			 VALUE "CompanyName",       APP_COMPANY
			 VALUE "FileVersion",       APP_FILEVER
			 VALUE "FileDescription",   APP_FILEDESC
			 VALUE "InternalName",      APP_INTERNALNAME
			 VALUE "LegalCopyright",    APP_COPYRIGHT
			 VALUE "LegalTrademarks",   APP_TRADEMARK
			 VALUE "OriginalFilename",  APP_ORIGINALNAME
			 VALUE "ProductName",       APP_PRODUCTNAME
			 VALUE "ProductVersion",    APP_PRODUCTVER
		 }
	 }
  BLOCK "VarFileInfo"
	 {
	     //-- language codes
		 VALUE "Translation", 0x0809, 1252 // English (United Kingdom)
	 }
}

