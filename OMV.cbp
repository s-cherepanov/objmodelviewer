<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Obj Model Viewer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="OpenGL\;OpenGL\;OpenGL\;OpenGL\Model\;OpenGL\;OpenGL\Model\Texture\;OpenGL\Widget\;OpenGL\;OpenGL\;Window\;System\;System\Resources\;System\Console\;" />
		<Option show_notes="1">
			<notes>
				<![CDATA[Project uses the following libraries:
-Qt 4.8.5 ( http://qt-project.org )
-glut 3.7.6 ( http://www.opengl.org/resources/libraries/glut )]]>
			</notes>
		</Option>
		<Build>
			<Target title="Debug">
				<Option output="Release\viewer - debug" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="Release\viewer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="Release (GUI)">
				<Option output="Release\viewer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="$(#qt4.include)" />
			<Add directory="$(#glut.include)" />
		</Compiler>
		<Linker>
			<Add option="-static" />
			<Add library="opengl32" />
			<Add library="glu32" />
			<Add library="$(#qt4.lib)\libQtCore4.a" />
			<Add library="$(#qt4.lib)\libQtGui4.a" />
			<Add library="$(#qt4.lib)\libQtOpenGL4.a" />
			<Add library="$(#glut.lib)\glut32.lib" />
		</Linker>
		<ExtraCommands>
			<Add before="$(#qt4)\bin\moc glwidget.h -o Generated\glwidget_moc.cpp" />
			<Add before="$(#qt4)\bin\uic window.ui -o Generated\window_ui.h" />
			<Add before="$(#qt4)\bin\moc window.h -o Generated\window_moc.cpp" />
			<Add before="$(#qt4)\bin\rcc resources.qrc -o Generated\resources.cpp" />
		</ExtraCommands>
		<Unit filename="Generated\glwidget_moc.cpp">
			<Option virtualFolder="OpenGL\Widget\" />
		</Unit>
		<Unit filename="Generated\resources.cpp">
			<Option virtualFolder="System\Resources\" />
		</Unit>
		<Unit filename="Generated\window_moc.cpp">
			<Option virtualFolder="Window\" />
		</Unit>
		<Unit filename="Generated\window_ui.h">
			<Option virtualFolder="Window\" />
		</Unit>
		<Unit filename="OMV.cpp">
			<Option virtualFolder="System\" />
		</Unit>
		<Unit filename="console.cpp">
			<Option virtualFolder="System\Console\" />
		</Unit>
		<Unit filename="console.h">
			<Option virtualFolder="System\Console\" />
		</Unit>
		<Unit filename="glm.cpp">
			<Option virtualFolder="OpenGL\Model\" />
		</Unit>
		<Unit filename="glm.h">
			<Option virtualFolder="OpenGL\Model\" />
		</Unit>
		<Unit filename="glmimg.cpp">
			<Option virtualFolder="OpenGL\Model\" />
		</Unit>
		<Unit filename="global.h">
			<Option virtualFolder="System\" />
		</Unit>
		<Unit filename="glwidget.cpp">
			<Option virtualFolder="OpenGL\Widget\" />
		</Unit>
		<Unit filename="glwidget.h">
			<Option virtualFolder="OpenGL\Widget\" />
		</Unit>
		<Unit filename="resources.qrc">
			<Option virtualFolder="System\Resources\" />
		</Unit>
		<Unit filename="resources.rc">
			<Option compilerVar="WINDRES" />
			<Option virtualFolder="System\Resources\" />
		</Unit>
		<Unit filename="texture.cpp">
			<Option virtualFolder="OpenGL\Model\Texture\" />
		</Unit>
		<Unit filename="texture.h">
			<Option virtualFolder="OpenGL\Model\Texture\" />
		</Unit>
		<Unit filename="window.cpp">
			<Option virtualFolder="Window\" />
		</Unit>
		<Unit filename="window.h">
			<Option virtualFolder="Window\" />
		</Unit>
		<Unit filename="window.ui">
			<Option virtualFolder="Window\" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
